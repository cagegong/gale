[
   {
      "lessonNo" : 119,
      "chinese" : "你喜欢听故事吗?",
      "sentenceNo" : 1,
      "timeline" : "00:00:00,499 --> 00:00:03,260",
      "english" : "Do you like stories?"
   },
   {
      "lessonNo" : 119,
      "chinese" : "我想要告诉你一个真实的故事.",
      "sentenceNo" : 2,
      "timeline" : "00:00:03,784 --> 00:00:07,325",
      "english" : "I want to tell you a true story."
   },
   {
      "lessonNo" : 119,
      "chinese" : "这是一年前发生在我的一个朋友身上的故事.",
      "sentenceNo" : 3,
      "timeline" : "00:00:08,088 --> 00:00:12,546",
      "english" : "It happened to a friend of mine a year ago."
   },
   {
      "lessonNo" : 119,
      "chinese" : "当我的朋George治在床上看书时,两个小偷爬进了他的厨房.",
      "sentenceNo" : 4,
      "timeline" : "00:00:12,997 --> 00:00:23,070",
      "english" : "While my friend, George, was reading in bed, two thieves climbed into his kitchen."
   },
   {
      "lessonNo" : 119,
      "chinese" : "他们进到屋里后,走进了饭厅.",
      "sentenceNo" : 5,
      "timeline" : "00:00:23,487 --> 00:00:29,618",
      "english" : "After they had entered the house, they went into the dining room."
   },
   {
      "lessonNo" : 119,
      "chinese" : "饭厅里很暗,于是他们打开了手电筒.",
      "sentenceNo" : 6,
      "timeline" : "00:00:29,634 --> 00:00:35,881",
      "english" : "It was very dark, so they turned on a torch."
   },
   {
      "lessonNo" : 119,
      "chinese" : "突然他们听到身后有声音.",
      "sentenceNo" : 7,
      "timeline" : "00:00:36,082 --> 00:00:41,139",
      "english" : "Suddenly, they heard a voice behind them."
   },
   {
      "lessonNo" : 119,
      "chinese" : "“什么事?什么事?”有人叫着.",
      "sentenceNo" : 8,
      "timeline" : "00:00:41,445 --> 00:00:45,405",
      "english" : "What's up? What's up?' someone called."
   },
   {
      "lessonNo" : 119,
      "chinese" : "小偷扔下了手电筒,以他们最快的速度逃走了.",
      "sentenceNo" : 9,
      "timeline" : "00:00:46,081 --> 00:00:54,002",
      "english" : "The thieves dropped the torch and ran away as quickly as they could."
   },
   {
      "lessonNo" : 119,
      "chinese" : "乔治听到了响声,迅速地下了楼.",
      "sentenceNo" : 10,
      "timeline" : "00:00:54,216 --> 00:00:59,816",
      "english" : "George heard the noise and came downstairs quickly."
   },
   {
      "lessonNo" : 119,
      "chinese" : "他开了灯,但不见一个人.",
      "sentenceNo" : 11,
      "timeline" : "00:00:59,929 --> 00:01:04,817",
      "english" : "He turned on the light, but he couldn't see anyone."
   },
   {
      "lessonNo" : 119,
      "chinese" : "小偷们已经逃走了.",
      "sentenceNo" : 12,
      "timeline" : "00:01:04,818 --> 00:01:08,045",
      "english" : "The thieves had already gone."
   },
   {
      "lessonNo" : 119,
      "chinese" : "但是George的鹦鹉Henry仍在那里.",
      "sentenceNo" : 13,
      "timeline" : "00:01:08,324 --> 00:01:13,048",
      "english" : "But George's parrot, Henry, was still there."
   },
   {
      "lessonNo" : 119,
      "chinese" : "“什么事,George?”它叫着.",
      "sentenceNo" : 14,
      "timeline" : "00:01:13,049 --> 00:01:16,036",
      "english" : "What's up, George?' he called."
   },
   {
      "lessonNo" : 119,
      "chinese" : "“没事,Henry.” George笑着说,",
      "sentenceNo" : 15,
      "timeline" : "00:01:16,224 --> 00:01:20,724",
      "english" : "Nothing, Henry,' George said and smiled."
   },
   {
      "lessonNo" : 119,
      "chinese" : "“接着睡觉吧.”",
      "sentenceNo" : 16,
      "timeline" : "00:01:20,810 --> 00:01:22,760",
      "english" : "Go back to sleep.'"
   }
]
