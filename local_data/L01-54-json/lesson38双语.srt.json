   },
   {
      "lessonNo" : 38,
      "chinese" : "什么不好?",
      "sentenceNo" : 11,
      "timeline" : "00:00:28,727 --> 00:00:28,991",
      "english" : "At what?"
   },
   {
      "lessonNo" : 38,
      "chinese" : "数学.",
      "sentenceNo" : 12,
      "timeline" : "00:00:30,274 --> 00:00:30,549",
      "english" : "Maths."
   },
   {
      "lessonNo" : 38,
      "chinese" : "你是说math?",
      "sentenceNo" : 13,
      "timeline" : "00:00:31,664 --> 00:00:32,254",
      "english" : "You mean math?"
   },
   {
      "lessonNo" : 38,
      "chinese" : "我们说maths.",
      "sentenceNo" : 14,
      "timeline" : "00:00:32,792 --> 00:00:33,335",
      "english" : "We say maths."
   },
   {
      "lessonNo" : 38,
      "chinese" : "你一定是指各种不同的数学.",
      "sentenceNo" : 15,
      "timeline" : "00:00:35,056 --> 00:00:36,631",
      "english" : "You must mean all the different maths."
   },
   {
      "lessonNo" : 38,
      "chinese" : "因为我指的是最基础的数学.",
      "sentenceNo" : 16,
      "timeline" : "00:00:36,954 --> 00:00:39,011",
      "english" : "Because I mean the one basic math."
   },
   {
      "lessonNo" : 38,
      "chinese" : "随便哪个都行.",
      "sentenceNo" : 17,
      "timeline" : "00:00:39,579 --> 00:00:39,857",
      "english" : "Whatever."
   },
   {
      "lessonNo" : 38,
      "chinese" : "但是Wendy, 听我说.",
